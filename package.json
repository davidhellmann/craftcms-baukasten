{
  "name": "craft-baukasten",
  "version": "1.1.3",
  "description": "Craft Baukasten Starter Kit",
  "keywords": [
    "Craft CMS",
    "Craft",
    "Webpack",
    "Vue",
    "Contentbuilder",
    "Scaffolding"
  ],
  "homepage": "https://github.com/example-developer/example-project",
  "bugs": {
    "url": "https://github.com/example-developer/example-project/issues"
  },
  "license": "MIT",
  "author": {
    "name": "David Hellmann",
    "email": "david@hellmann.io",
    "url": "https://davidhellmann.com"
  },
  "browser": "/web/index.php",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/example-developer/example-project.git"
  },
  "private": true,
  "scripts": {
    "upgrade-interactive": "npm-check --update",
    "debug": "webpack-dev-server --config webpack.dev.js",
    "twc": "npx tailwindcss build src/css/app.scss -o src/css/tailwind.output.css -c tailwind.config.js",
    "dev": "npm run twc && webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress --hide-modules",
    "preinstall": "$npm_execpath --silent run checknpm",
    "checknpm": "echo \"$npm_execpath\" | grep -q \"npm-cli\\.js$\" || (echo '⚠️ Use npm not yarn! No Hipster Party here!! ⚠️ ' && echo && exit 1)",
    "eslint": "eslint src/**/*.{js,vue} --fix",
    "stylelint": "stylelint src/**/*.scss --fix",
    "cy:run": "cypress run"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "legacyBrowsers": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR"
    ],
    "modernBrowsers": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./eslint.config.js"
    ]
  },
  "eslintIgnore": [
    "/node_modules/**",
    "/modules/**",
    "/storage/**",
    "/vendor/**",
    "/src/fonts/**",
    "/src/inlineJs/**",
    "/src/ferdi/**",
    "/web/**",
    "/src/js/workbox-catch-handler.js"
  ],
  "stylelint": {
    "extends": [
      "./stylelint.config.js"
    ],
    "ignoreFiles": [
      "node_modules/**/*",
      "modules/**/*",
      "storage/**/*",
      "vendor/**/*",
      "src/ferdi/**/*",
      "src/fonts/**/*",
      "src/css/ITCSS/01-settings/**/*",
      "src/css/ITCSS/02-tools/**/*",
      "src/css/ITCSS/08-vendors/**/*",
      "src/vendor/**/*",
      "web/**/*"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 200
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@gfx/zopfli": "^1.0.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.2",
    "create-symlink-webpack-plugin": "^1.0.0",
    "critical": "^2.0.5",
    "critical-css-webpack-plugin": "^2.0.0",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.0",
    "cypress": "^4.12.1",
    "cypress-localstorage-commands": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.1",
    "file-loader": "^5.0.2",
    "git-rev-sync": "^2.1.0",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.3",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.29.1",
    "npm-check": "^5.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.1",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.2",
    "save-remote-file-webpack-plugin": "^1.0.0",
    "style-loader": "^1.3.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "svgo": "^1.3.2",
    "symlink-webpack-plugin": "^0.2.0",
    "terser": "^4.8.0",
    "terser-webpack-plugin": "^2.3.8",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webapp-webpack-plugin": "git+https://github.com/brunocodutra/webapp-webpack-plugin.git",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.2.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4",
    "webpack-notifier": "^1.6.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "babel-polyfill": "^6.26.0",
    "baukasten-grid": "^4.0.3",
    "bowser": "^2.11.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "css-vars-ponyfill": "^2.3.2",
    "family.scss": "^1.0.8",
    "ferdi": "0.0.21",
    "gsap": "^3.5.1",
    "include-media": "^1.4.9",
    "lazysizes": "^5.2.2",
    "lodash": "^4.17.20",
    "macy": "^2.5.1",
    "modularscale-sass": "^3.0.10",
    "photoswipe": "^4.1.3",
    "regenerator-runtime": "^0.13.7",
    "smooth-scroll": "^16.1.1",
    "swiper": "^6.3.3",
    "tailwindcss": "^1.9.4",
    "typeface-lato": "0.0.75",
    "typeface-merriweather": "0.0.72",
    "typeface-roboto-mono": "0.0.75",
    "vue": "^2.6.12",
    "vue-axios": "^2.1.5",
    "vue-confetti": "^2.2.1",
    "vue-router": "^3.4.7",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  }
}
